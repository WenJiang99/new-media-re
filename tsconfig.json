{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "typeRoots": [
      "node_modules/@types",
      "global.d.ts"
    ],
    "baseUrl": ".",
    "paths": {
    }
  },
  "exclude": [
    "dist",
    "node_modules",
    "scripts",
    "config"
  ],
  "compileOnSave": true
}